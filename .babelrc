{
    "presets": [
        [
            "@babel/preset-env",
            {
                "useBuiltIns": "usage",
                "corejs": {
                    "version" : "3",
                    "proposals" : true,
                    "debug": true
                },
                "debug" : true,
                "modules": false,
                "exclude": [
                    "es.string.match",
                    "es.string.replace",
                    "es.array.reduce",
                    "es.array.iterator",
                    "es.string.trim",
                    "web.dom-collections.iterator"
                ]
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-transform-object-assign",
        "@babel/plugin-transform-parameters"
    ]
}
